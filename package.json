{
  "name": "choonkiatlee.github.io",
  "version": "3.0.1",
  "license": "MIT",
  "description": "A personal website for note-taking built based on 11ty engine with high performance.",
  "scripts": {
    "build": "rm -rf _site _live && npm run js-build && eleventy && echo \"Starting copy...\" && mkdir -p _live && cp -Rf _site/* _live/ && echo \"End copy.\"",
    "build-old": "npm run js-build && eleventy && npm run test",
    "watch": "concurrently \"eleventy --serve\" \"npm run js-build-watch\" \"npm run test-watch\"",
    "local-watch": "ELEVENTY_ENV=local concurrently \"eleventy --serve --quiet\" \"npm run js-build-watch\"",
    "local-build": "rm -rf _site _live && npm run js-build && ELEVENTY_ENV=local eleventy --quiet && echo \"Starting copy...\" && mkdir -p _live && cp -Rf _site/* _live/ && echo \"End copy.\"",
    "local-serve": "echo \"Starting copy...\" && mkdir -p _live && cp -Rf _site/* _live/ && echo \"End copy.\" && http-server _live",
    "js-build": "rollup -c rollup.config.js",
    "js-build-watch": "rollup -c rollup.config.js -w",
    "debug": "DEBUG=* eleventy",
    "test": "mocha src/test/test*.js",
    "test-watch": "mocha src/test/test*.js --watch"
  },
  "pre-push": [],
  "pre-commit": [],
  "repository": {
    "type": "git",
    "url": "git://github.com/choonkiatlee/choonkiatlee.github.io"
  },
  "bugs": {
    "url": "https://github.com/choonkiatlee/choonkiatlee.github.io/issues"
  },
  "homepage": "https://choonkiatlee.github.io",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@11ty/eleventy-plugin-rss": "^1.0.7",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.1",
    "@gerhobbelt/markdown-it-inline-text-color": "^1.0.1-1",
    "elasticlunr": "^0.9.5",
    "eleventy-plugin-local-images": "^0.4.0",
    "eleventy-plugin-toc": "^1.1.0",
    "file-type": "^12.0.1",
    "fs-extra": "^8.1.0",
    "jsdom": "^15.1.1",
    "katex": "^0.12.0",
    "luxon": "^1.21.3",
    "markdown-it-anchor": "^5.2.5",
    "markdown-it-container": "^3.0.0",
    "markdown-it-footnote": "^3.0.2",
    "markdown-it-kbd": "^2.2.0",
    "markdown-it-task-lists": "^2.1.1",
    "node-fetch": "^2.6.1",
    "pre-commit": "^1.2.2",
    "pre-push": "^0.1.1",
    "rollup-plugin-scss": "^2.6.1",
    "rollup-plugin-terser": "^6.1.0",
    "shorthash": "0.0.2"
  },
  "dependencies": {
    "@ampproject/toolbox-optimizer": "^1.1.2",
    "@saschazar/wasm-avif": "^1.0.0",
    "clean-css": "^4.2.3",
    "concurrently": "^5.2.0",
    "copyfiles": "^2.4.1",
    "csp-hash-generator": "0.0.3",
    "csso": "^4.0.3",
    "datauri": "^3.0.0",
    "expect.js": "^0.3.1",
    "hasha": "^5.2.0",
    "html-minifier": "^4.0.0",
    "image-size": "^0.8.3",
    "lru-cache": "^5.1.1",
    "markdown-it": "^11.0.1",
    "markdown-it-attrs": "^3.0.3",
    "markdown-it-emoji": "^2.0.0",
    "markdown-it-mark": "^3.0.0",
    "markdown-it-table-of-contents": "^0.5.0",
    "mocha": "^8.1.3",
    "phin": "^3.5.0",
    "purge-from-html": "^1.0.3",
    "purgecss": "^2.3.0",
    "querystring": "^0.2.0",
    "rollup": "^2.23.0",
    "sharp": "^0.25.4",
    "twitter": "^1.7.1"
